          "data": [
            {
              /*Long vector message from 0xAA to 0xCC with 0x10C words of payload
              across two vectors. It calls handler 0xF with zero handler args. First 
              vector has 0xC words to address 0 and second has has 0x100 words to 
              address 0xFF00*/
              "type": "axis", "interface": "axis_net", "width": 32, "id": "LMC-I-H",
              "payload": [
                {"data": "{AMHeader,type:0x6,src:0xAA,dst:0xCC,payload:0x10C,handler:0xF,args:0}",
                  "keep": "ALL","last": 0,"callTB": 1},
                {"data": "{AMLongVector,dst:2,dstSize:0xC,token:0x6}","keep": "ALL","last": 0,
                  "callTB": 0},
                {"data": "0x0","keep": "ALL","last": 0,"callTB": 0},
                {"data": "0x100","keep": "ALL","last": 0,"callTB": 0},
                {"data": "0xFF00","keep": "ALL","last": 0,"callTB": 1},
                {"loop": { "start": 0, "end": 3, "index": "i", "body": [
                  {"loop": { "start": 0, "end": 3, "index": "j", "body": [
                    //Payload 0-2, 4-6, 8-10
                    {"data": "0xBEEFDEAD","keep": "ALL","last": 0,"callTB": 0}
                  ]}},
                  //Payload 3, 7, 11
                  {"data": "0xDEADBEEF","keep": "ALL","last": 0,"callTB": 0}
                ]}},
                {"loop": { "start": 0, "end": 255, "index": "i", "body": [
                  //Payload 0-254
                  {"data": "0xFFAAFFAA","keep": "ALL","last": 0,"callTB": 0}
                ]}},
                //Payload 255
                {"data": "0xDDDDDDDD","keep": "ALL","last": 1,"callTB": 1},
                {"data": "0sdbgContinue","keep":0,"last":0,"callTB":1}
              ]
            }
          ]
        },{
          "data": [
            {
              //send status good
              "type": "axis", "interface": "axis_s2mmStatus", "width": 8, 
                "id": "LMC-I-S",
